                                                                </ul>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </td>
                                            </tr>
                                            break;
                                        default:
                                            <tr class="table-light">
                                                <td class="text-center">
                                                    <span class="fw-bold" title="@project.Number">@project.Number</span>
                                                </td>
                                                <td class="text-center">
                                                    <span class="fw-bold" title="@project.Description">@project.Title</span>
                                                </td>
                                                <td class="text-center">
                                                    <i title="In Progress" class="badge rounded-pill fw-bold text-white" style="font-size: 1.2rem; background-color: #27ae60; width:150px" />
                                                </td>
                                                <td class="text-center" title="@project.AssignedToName">
                                                    <i class="bi bi-person fw-bold" title="@project.AssignedToName" style="font-size: 1.4rem;"></i>
                                                </td>
                                                <td title="@project.ExpectedDueTime.Value.ToLongDateString()">
                                                    <span class="fw-bold">@project.ExpectedDueTime.Value.ToString("dd-MM-yyyy")</span>
                                                </td>
                                                <td class="text-center">
                                                    <!-- Default dropstart button -->
                                                    <div class="btn-group dropstart">
                                                        <button type="button" data-bs-toggle="dropdown" aria-expanded="false" style="border:0px">
                                                            <span class="fw-bold" style="font-size:1.0rem;"><i class="bi bi-three-dots-vertical"></i></span>
                                                        </button>
                                                        <ul class="dropdown-menu dropdown-menu-dark" aria-labelledby="dropdownMenuButton2">

                                                            <li>
                                                                <a class="dropdown-item" asp-area="WKS" asp-controller="Projects" asp-action="ProjectInfo" asp-route-id="@project.ID" title="Details" class="btn btn-sm btn-dark">
                                                                    <i class="bi bi-info-lg"></i>  <span>Details</span>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a class="dropdown-item" asp-area="WKS" asp-controller="Projects" asp-action="Manage" asp-route-id="@project.ID" class="btn btn-sm btn-info text-white" style="width:75px" title="Edit Folder">
                                                                    <i class="bi bi-pencil-fill"></i>  <span>Edit</span>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a class="dropdown-item" asp-area="WKS" asp-controller="Projects" asp-action="MoreInfo" asp-route-id="@project.ID" title="Add More Info">
                                                                    <i class="bi bi-trash-fill"></i>  <span>More Info</span>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a class="dropdown-item" asp-area="WKS" asp-controller="Projects" asp-action="Delete" asp-route-id="@project.ID" class="btn btn-sm btn-danger text-white" style="width:75px" title="Delete Folder">
                                                                    <i class="bi bi-trash-fill"></i>  <span>Delete</span>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </td>
                                            </tr>
                                            break;
                                    }
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        }
    }
</div>
@section Scripts{
    <script src="~/js/js_asm/asm_master_assets.js"></script>
}










