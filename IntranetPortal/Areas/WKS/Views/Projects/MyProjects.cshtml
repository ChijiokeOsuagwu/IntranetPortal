@using IntranetPortal.Base.Enums;
@model IntranetPortal.Areas.WKS.Models.ProjectListViewModel
@{
    ViewData["Title"] = "OfficeManager | My Projects";
}
<div class="container">
    <h4>@Model.FolderName</h4>
    <hr />
    <div class="mb-1">
        <form asp-area="WKS" asp-controller="Projects" asp-action="MyProjects" asp-route-id="@Model.id" method="get">
            <div class="row">
                <div class="col-md-2">
                    @if (Model.Source == "xws")
                    {
                        <a asp-area="WKS" asp-controller="Home" asp-action="ExecutiveWorkspace" class="btn btn-dark" style="width:100px" title="Click to Go Back">
                            <span class="text-white">Back</span>
                        </a>
                    }
                    else
                    {
                        <a asp-area="WKS" asp-controller="Home" asp-action="ExecutiveWorkspace" class="btn btn-dark" style="width:100px" title="Click to Go Back">
                            <span class="text-white">Back</span>
                        </a>
                    }
                </div>
                <div class="col-md-6 text-end">
                    <input asp-for="sp" placeholder="Enter Title " class="border-0" style="width:300px" />
                    <input type="submit" value="Search" class="btn btn-dark" style="width:100px">
                </div>
                <div class="col-md-4 text-end">
                    <a asp-area="WKS" asp-controller="Projects" asp-action="Manage" asp-route-fd="@Model.id" class="btn btn-success" style="width:150px" title="Add New Project">
                        <i class="bi bi-plus-lg text-white fw-bold"></i>  <span class="text-white ml-2">New Project</span>
                    </a>
                </div>
            </div>
        </form>
    </div>

    @if (!String.IsNullOrEmpty(ViewBag.ErrorMessage))
    {
        <div class="alert alert-danger alert-dismissible fade show" role="alert">
            <i class="bi bi-x-circle"></i>
            <span class="fw-bold">@ViewBag.ErrorMessage</span>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    }
    @if (Model.ProjectList == null || Model.ProjectList.Count < 1)
    {
        <div class="alert alert-info fade show" role="alert">
            <i class="bi bi-info-circle"></i>
            <span class="fw-bold">No record was found.</span>
        </div>
    }
